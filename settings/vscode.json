        "vs-kubernetes.minikube-path": "/Users/mike/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.minikube-path.mac": "/Users/mike/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
